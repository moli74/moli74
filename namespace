#include<iostream>
using namespace std;
class Date//Date
{
public:
	Date(int year=1, int month=1, int day=1)//构造函数
	{
		_year = year;
		_month = month;
		_day = day;
	}
	void Print()
	{
		cout << _year << "年" << _month << "月" << _day << "天" << endl;
	}
private:
	int _year;
	int _month;
	int _day;
};
int main()
{
	Date d1(1,1,2);
	d1.Print();
	return 0;
}
